{
  "name": "tocco-merge",
  "version": "0.2.1",
  "description": "merge action",
  "main": "dist/index.js",
  "scripts": {
    "clean": "../../node_modules/rimraf/bin.js dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "cd ../../ && ./node_modules/better-npm-run/index.js deploy --package=merge",
    "prepublish": "cd ../../ && ./node_modules/better-npm-run/index.js deploy:prod --package=merge"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=merge",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "moment": "2.17.1",
    "tocco-test-util": "^0.1.0",
    "tocco-theme": "0.1.0",
    "tocco-ui": "^0.1.0",
    "tocco-util": "^0.1.0"
  }
}
